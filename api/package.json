{
  "name": "@template/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "predev": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "premigration:generate": "yarn build",
    "premigration:create": "yarn build",
    "premigration:run": "yarn build",
    "premigration:revert": "yarn build",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "conf": "cp -r ../shared/config/ ./src/"
  },
  "workspaces": {
    "nohoist": [
      "typeorm",
      "typeorm/**"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/typeorm": "^6.2.0",
    "@template/shared": "1.0.0",
    "@types/passport-jwt": "^3.0.3",
    "apollo-server-express": "^2.9.16",
    "bcrypt": "^3.0.8",
    "dateformat": "^3.0.3",
    "generate-password": "^1.5.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
